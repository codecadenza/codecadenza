<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{dd.MM.yyyy HH:mm:ss,SSS} %-5p %c (%L) - %m%n"/>
		</Console>
	</Appenders>

	<Loggers>
		<Logger name="net.codecadenza.runtime" level="DEBUG"/>
	</Loggers>

</Configuration>
